{
  "name": "react-facade",
  "description": "A simple library that uses Proxy and TypeScript to create a strongly typed facade for your React hooks.",
  "version": "0.3.4",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "license": "MIT",
  "author": "Gabe Scholz <github@gabe.pizza>",
  "files": [
    "build",
    "src",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "invariant": "^2.2.4"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/invariant": "^2.2.35",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "happy-dom": "^6.0.2",
    "npm-check-updates": "^15.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vitest": "^0.17.0"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup src/index.ts --clean --outDir build --dts --sourcemap --format esm,cjs",
